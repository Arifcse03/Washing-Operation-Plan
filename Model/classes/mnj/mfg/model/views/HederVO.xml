  <SQLQuery>
    <![CDATA[SELECT HederEO.GWP_GE_ID,
         HederEO.GWP_GE_NUMBER,
         HederEO.GE_DATE,
         HederEO.SALESORDER_ID,
         HederEO.DOUBLE_WASH,
         HederEO.AVG_GARMENT_DAY,
         HederEO.REMARKS,
         HederEO.LAST_UPDATE_DATE,
         HederEO.LAST_UPDATED_BY,
         HederEO.CREATION_DATE,
         HederEO.CREATED_BY,
         HederEO.GE_STATUS,
         HederEO.SO_HEADER_ID,
         HederEO.ORG_ID,
         HederEO.Customer_Id,
         HederEO.Quantity,
         (SELECT ooha.ORDER_NUMBER
            FROM oe_order_headers_all ooha
           WHERE ooha.HEADER_ID = HederEO.SALESORDER_ID)
            order_number,
         (SELECT c.CUSTOMER_NAME
            FROM ar_customers c
           WHERE c.CUSTOMER_ID = HederEO.Customer_Id)
            customer_name,
         HederEO.SEASON,
         HederEO.BRAND,
         HederEO.DIVISION,
         HederEO.STYLE,
         HederEO.WASH,
         HederEO.VERSION_NO,
         HederEO.VERSION_DATE,
         CASE
            WHEN HederEO.VERSION_NO IS NULL THEN 'Original'
            ELSE TO_CHAR (HederEO.VERSION_NO)
         END
            VARSION_NEW,
         (                                           --   SELECT prha.segment1
          --    FROM po_requisition_headers_all prha
          --    WHERE prha.org_id = HederEO.ORG_ID
          --    and prha.attribute14 = to_char(HederEO.GWP_GE_ID)
          --    AND ROWNUM = 1
          --
          SELECT NULL FROM DUAL) PR_Num,
         HederEO.Color,
         /*case statement added by Mr. Sakibul Islam on requiremnet of Mr. Wasi Udin for showing finish_goods_color on 25.Mar.2020*/
         (SELECT CASE
                    WHEN    oola.ATTRIBUTE13 IS NULL
                         OR oola.ATTRIBUTE13 = 'No entry'
                    THEN
                       TRIM (
                          (SELECT FFV.DESCRIPTION
                             FROM FND_FLEX_VALUES_VL FFV
                            WHERE     FFV.FLEX_VALUE = msib.segment4
                                  AND FFV.FLEX_VALUE_SET_ID = 1014895
                                  AND FFV.ENABLED_FLAG = 'Y'
                                  AND ROWNUM = 1         -- added on 27 nov 19
                                                ))
                    ELSE
                       oola.ATTRIBUTE13
                 END
                    Finish_Goods_Color
            FROM OE_ORDER_HEADERS_ALL OOHA,
                 OE_ORDER_LINES_ALL OOLA,
                 MTL_SYSTEM_ITEMS MSIb
           WHERE     1 = 1
                 AND OOHA.HEADER_ID = OOLA.HEADER_ID
                 AND MSIb.INVENTORY_ITEM_ID = OOLA.INVENTORY_ITEM_ID -- added on 18 feb 20
                 AND MSIb.ORGANIZATION_ID = 353
                 AND ooha.SHIPPING_INSTRUCTIONS = HederEO.STYLE     --'569350'
                 AND ooha.ATTRIBUTE4 = HederEO.SEASON           --'C3-20 Fall'
                 AND ooha.SOLD_TO_ORG_ID = HederEO.Customer_Id        --'1052'
                 AND msib.attribute17 = HederEO.WASH         --'BB MH Table#1'
                 AND ROWNUM = 1)
            Finish_Goods_Color,
         HederEO.Transaction_Date,
         HederEO.WASH_CATAGORY,
         (SELECT DECODE (StyleLineEO.TRACK_BASIS,
                         'Style Name', StyleLineEO.STYLE_NAME,
                         'Style Number', StyleLineEO.STYLE_NUMBER)
            FROM PWC_MFG_BSS_STYLE_L StyleLineEO,
                 pwc_mfg_bss_buyer_h pwcmfgbssbuyerh
           WHERE     StyleLineEO.STYLE_IDENTIFIER = HederEO.Style --HeaderEO.STYLE
                 AND pwcmfgbssbuyerh.BUYER_ID = HederEO.Customer_Id --HeaderEO.BUYER_ID
                 --AND   pwcmfgbssbuyerh.ORG_ID = HeaderEO.ORG_ID
                 AND StyleLineEO.HEADER_ID = pwcmfgbssbuyerh.HEADER_ID
                 AND ROWNUM = 1)
            CURRENT_STYLE,
         (SELECT SeasonDetailEO.SEASON_NAME
            FROM PWC_MFG_BSS_STYLE_L StyleLineEO,
                 pwc_mfg_bss_buyer_h pwcmfgbssbuyerh,
                 PWC_MFG_BSS_SEASON_D SeasonDetailEO
           WHERE     StyleLineEO.STYLE_IDENTIFIER = HederEO.Style --HeaderEO.STYLE
                 AND SeasonDetailEO.SEASON_IDENTIFIER = HederEO.Season --HeaderEO.SEASON
                 AND pwcmfgbssbuyerh.BUYER_ID = HederEO.Customer_Id --HeaderEO.BUYER_ID
                 --AND   pwcmfgbssbuyerh.ORG_ID = HeaderEO.ORG_ID
                 AND StyleLineEO.HEADER_ID = pwcmfgbssbuyerh.HEADER_ID
                 AND SeasonDetailEO.LINE_ID = StyleLineEO.LINE_ID
                 AND ROWNUM = 1)
            CURRENT_SEASON,
         (SELECT WIP_ENTITY_NAME job_name
            FROM wip_discrete_jobs_v d
           WHERE     1 = 1
                 --    and d.attribute3 = :header_id
                 --AND d.attribute2 = j1_rec.DESCRIPTION||'-'||j1_rec.SEASON
                 AND d.attribute6 = H.STYLE
                 AND d.attribute7 = H.SEASON
                 AND d.attribute8 = l.COLOR
                 AND d.attribute9 = l.WASH_Name
                 AND d.Status_Type NOT IN (7, 12)
                 AND d.class_code = 'WASH'
                 AND d.attribute_category = 'WASHING'
                 --and  HederEO.ORG_ID = d.ORGANIZATION_ID   --commented on --29.aug.2018 as org id != organization id

                 AND d.ORGANIZATION_ID =
                        (SELECT organization_id
                           FROM org_organization_definitions
                          WHERE operating_unit = HederEO.ORG_ID))
            JOB_NAME,
         (  SELECT SUM (OL.ORDERED_QUANTITY) ORDERED_QTY
              FROM OE_ORDER_HEADERS_ALL OH, OE_ORDER_LINES_ALL OL
             WHERE     1 = 1
                   AND OH.HEADER_ID = OL.HEADER_ID
                   AND OH.SOLD_TO_ORG_ID = HEDEREO.CUSTOMER_ID
                   AND OH.ATTRIBUTE4 = HEDEREO.SEASON
                   AND OH.ATTRIBUTE1 = HEDEREO.STYLE
          GROUP BY OH.SOLD_TO_ORG_ID, OH.ATTRIBUTE4, OH.ATTRIBUTE1 -- order by oh.sold_to_org_id ,  oh.attribute4 , oh.attribute1
                                                                  )
            ORDERED_QTY,
            HederEO.SYSTEM_ID,
            H.poc_id,
            
            ac.CUSTOMER_NAME BUYER_NAME_NEW,
            H.SEASON SEASON_NEW,
            H.STYLE STYLE_NEW,
            
            L.color  COLOR_NEW,
            L.wash_name wash_NEW
    FROM MNJ_GWP_MFG_GEVALUATION HederEO,xx_om_poc_h_t H, xx_om_poc_L_t L, ar_customers ac
   WHERE HederEO.ORG_ID = :p_org
   and h.poc_id = L.poc_id
   and L.System_id=HederEO.SYSTEM_ID
AND h.BUYER_ID = ac.CUSTOMER_ID

ORDER BY HederEO.GWP_GE_ID DESC]]>
  </SQLQuery>
